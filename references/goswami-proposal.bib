@ARTICLE{bonnechere,

AUTHOR={Bonnechère, Bruno  and Amin, Najaf  and van Duijn, Cornelia },

TITLE={The Role of Gut Microbiota in Neuropsychiatric Diseases – Creation of An Atlas-Based on Quantified Evidence},

JOURNAL={Frontiers in Cellular and Infection Microbiology},

VOLUME={12},

YEAR={2022},

URL={https://www.frontiersin.org/journals/cellular-and-infection-microbiology/articles/10.3389/fcimb.2022.831666},

DOI={10.3389/fcimb.2022.831666},

ISSN={2235-2988},

ABSTRACT={<p>There is a growing body of evidence highlighting the significant role of gut microbiota in various pathologies. We performed a systematic review to review the different microbiota involved in neuropsychiatric diseases. 50 studies (23 studies for autism spectrum disorders, 18 for major depression, and 9 for schizophrenia), representing 2,137 patients and 2,844 controls. Concerning the microbiota, the genera <italic>Prevotella, Clostridium, Bacteroides, Bifidobacterium, Ruminococcus, Megamonas, and Faecalbacterium</italic> were the ones detected with the most frequent variation of their relatives abundance. We also assess the overlap between the different pathologies. This study provides new insights into the complex relationship between the brain and the gut and the implications in neuropsychiatric pathologies. The identification of unique signatures in neuropsychiatric diseases suggests new possibilities in targeted anti or probiotic treatment.</p>}}

